{
      title: 'Action',
      field: 'jobAction',
      headerHozAlign: 'center',
      headerSort: false,
      width: 150,
      editor: 'list',
      editable: false,
      // editable: function (cell) {
      //   return !readOnly;
      // },
      editorParams: function (cell) {
        // 상태값 정리 (지우지 마세요.)
        // if(PDK)
        //   { value: 'ACTION_DISABLE', label: 'Disable' },
        //   { value: 'ACTION_RESET_CHECKSUM', label: 'Reset Checksum' },
        //   { value: 'ACTION_ENABLE', label: 'Fix files'}
        //   { value: 'ACTION_RE_DOWNLOAD', label: 'Re-Download' },
        //   { value: 'ACTION_MOVE', label: 'Move' },
          
        // if(IAD)
        //   { value: 'ACTION_DISABLE', label: 'Disable' },
        //   { value: 'ACTION_ENABLE', label: 'Fix files'}
        //   { value: 'ACTION_ENABLE', label: 'Enable' },
        //   { value: 'ACTION_RE_DOWNLOAD', label: 'Re-Download' },
        //   { value: 'ACTION_MOVE', label: 'Move' },
        const row = cell.getRow().getData();
        let valuesArray = [];
        if (dataType === 'IAD') {
          switch (row.status.code) {
            case 'MANUALLY_ADDED':
            case 'CHKSUMERROR':
              return {
                values: [{ value: 'ACTION_DISABLE', label: 'Disable' }],
              };
            case 'MODIFIED':
              return {
                values: [
                  { value: 'ACTION_DISABLE', label: 'Disable' },
                  { value: 'ACTION_RESET_CHECKSUM', label: 'Fix files' },
                ],
              };
            case 'DELETED':
              return {
                values: [
                  { value: 'ACTION_DISABLE', label: 'Disable' },
                  { value: 'ACTION_ENABLE', label: 'Enable' },
                ],
              };
            case 'MANUALLY_DISABLED':
              return {
                values: [{ value: 'ACTION_ENABLE', label: 'Enable' }],
              };
          }
        } else {
          //dataType === 'PDK'
          switch (row.status.code) {
            case 'MANUALLY_ADDED':
            case 'DOWNLOADED':
            // case 'DOWNLOAD_FAILED':
            // case 'MODIFIED':
            // case 'DELETED':
            // case 'SKIPPED':
            case 'NOTSERVICED':
            case 'CHKSUMERROR':
              return {
                values: [{ value: 'ACTION_DISABLE', label: 'Disable' }],
              };
            case 'MANUALLY_DISABLED':
            case 'AUTO_DISABLED':
            case 'ERROR_DISABLED':
              return {
                values: [{ value: 'ACTION_ENABLE', label: 'Fix files' }],
              };
            case 'MODIFIED':
            case 'DELETED':
            case 'SKIPPED':
              return {
                values: [
                  { value: 'ACTION_DISABLE', label: 'Disable' },
                  { value: 'ACTION_ENABLE', label: 'Fix files' },
                ],
              };
            case 'DOWNLOAD_FAILED':
              return {
                values: [
                  { value: 'ACTION_DISABLE', label: 'Disable' },
                  { value: 'ACTION_ENABLE', label: 'Fix files' },
                  { value: 'ACTION_RE_DOWNLOAD', label: 'Re-Download' },
                ],
              };
            case 'TEMP_DOWNLOADED':
              return {
                values: [{ value: 'ACTION_MOVE', label: 'Move' }],
              };
          }
        }
      },
      formatter: function (cell) {
        // 화면상의 value 값을 label로 표현하기 위함.
        const value = cell.getValue();
        // editorParams에서 생성한 values를 가져와서 룩업(lookup)
        let editorValues = [
          { value: 'ACTION_DISABLE', label: 'Disable' },
          { value: 'ACTION_RESET_CHECKSUM', label: 'Fix files' },
          { value: 'ACTION_ENABLE', label: dataType === 'IAD' ? 'Enable' : 'Fix files' },
          { value: 'ACTION_RE_DOWNLOAD', label: 'Re-Download' },
          { value: 'ACTION_MOVE', label: 'Move' },
        ];
        const foundItem = editorValues.find((item) => item.value === value);
        return foundItem ? foundItem.label : '...';
      },
    }, 

이렇게 tabulator 컬럼을 정의했는데 editable을 false로 해도 자꾸 리스트가 활성화되는 문제가 있음
